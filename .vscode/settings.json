{
    "files.associations": {
        "*.abi": "json",
        "*.css": "css",
        "ostream": "cpp",
        "cmath": "cpp",
        "complex": "cpp",
        "chrono": "cpp",
        "vector": "cpp",
        "iostream": "cpp"
    },
    "C_Cpp.errorSquiggles": "disabled",
    "code-runner.executorMap": {
